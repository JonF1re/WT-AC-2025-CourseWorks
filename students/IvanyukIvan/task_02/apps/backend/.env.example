# Backend environment variables
NODE_ENV=development
PORT=3000
CORS_ORIGIN=http://localhost:5173
DATABASE_URL="postgresql://postgres:PASSWORD@HOST:5432/progress_tracker?schema=public"

# JWT secrets and TTLs
JWT_ACCESS_SECRET=replace_with_strong_access_secret_at_least_32_chars
JWT_REFRESH_SECRET=replace_with_strong_refresh_secret_at_least_32_chars
JWT_ACCESS_TTL=15m
JWT_REFRESH_TTL=7d
